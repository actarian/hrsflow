{
	"targets": {
		"browser": {
			"compile": [{
				"inputFile": "src/sass/bootstrap.scss",
				"outputFile": "docs/css/bootstrap.css"
			}, {
				"inputFile": "src/sass/app.scss",
				"outputFile": "docs/css/app.css"
			}, {
				"inputFile": "src/app/app.js",
				"outputFile": "docs/js/app.js"
			}],
			"bundle": [{
				"inputFiles": [
					"docs/js/app.js"
				],
				"outputFileName": "docs/js/app.js",
				"minify": {
					"enabled": true
				}
			}, {
				"inputFiles": [
					"docs/css/app.css"
				],
				"outputFileName": "docs/css/app.css",
				"minify": {
					"enabled": true
				}
			}, {
				"inputFiles": [
					"node_modules/jquery/dist/jquery.js",
					"node_modules/@fancyapps/fancybox/dist/jquery.fancybox.js",
					"node_modules/gsap/src/uncompressed/easing/EasePack.js",
					"node_modules/gsap/src/uncompressed/TweenLite.js",
					"node_modules/gsap/src/uncompressed/TweenMax.js",
					"node_modules/swiper/dist/js/swiper.js"
				],
				"outputFileName": "docs/js/vendors.js",
				"minify": {
					"enabled": true
				}
			}, {
				"inputFiles": [
					"node_modules/@fancyapps/fancybox/dist/jquery.fancybox.css",
					"node_modules/swiper/dist/css/swiper.css"
				],
				"outputFileName": "docs/css/vendors.css",
				"minify": {
					"enabled": true
				}
			}, {
				"inputFiles": [
					"docs/css/bootstrap.css"
				],
				"outputFileName": "docs/css/bootstrap.css",
				"minify": {
					"enabled": true
				}
			}]
		}
	},
	"options": {
		"tfs": false
	}
}
