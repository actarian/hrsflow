{
	"targets": {
		"browser": {
			"compile": [{
				"input": "src/sass/bootstrap.scss",
				"output": "docs/css/bootstrap.css"
			}, {
				"input": "src/sass/app-ww.scss",
				"output": "docs/css/app-ww.css",
				"minify": false
			}, {
				"input": "src/sass/app-ch.scss",
				"output": "docs/css/app-ch.css",
				"minify": false
			}, {
				"input": "src/app/app.js",
				"output": "docs/js/app.js",
				"minify": true
			}],
			"bundle": [{
				"input": [
					"node_modules/jquery/dist/jquery.js",
					"node_modules/@fancyapps/fancybox/dist/jquery.fancybox.js",
					"node_modules/gsap/src/uncompressed/easing/EasePack.js",
					"node_modules/gsap/src/uncompressed/TweenLite.js",
					"node_modules/gsap/src/uncompressed/TweenMax.js",
					"node_modules/swiper/dist/js/swiper.js"
				],
				"output": "docs/js/vendors.js",
				"minify": true
			}, {
				"input": [
					"node_modules/@fancyapps/fancybox/dist/jquery.fancybox.css",
					"node_modules/swiper/dist/css/swiper.css"
				],
				"output": "docs/css/vendors.css",
				"minify": true
			}, {
				"input": [
					"docs/css/bootstrap.css"
				],
				"output": "docs/css/bootstrap.css",
				"minify": true
			}]
		}
	},
	"options": {
		"tfs": false
	}
}
